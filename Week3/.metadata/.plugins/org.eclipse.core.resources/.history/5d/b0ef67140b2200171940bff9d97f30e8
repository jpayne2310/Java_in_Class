import java.util.Scanner;
public class SalesRun {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner keyboard = new Scanner(System.in);
		String firstName = "";
		String lastName = "";
		String space = "\n";
		 
		
		System.out.print("Enter the first name of the Salesperons: ");
		firstName = keyboard.nextLine();
		System.out.print("Enter the last name of the Salesperons: ");
		lastName = keyboard.nextLine();
		
		SalesPerson salesPerson1 = new SalesPerson(firstName, lastName);
		salesPerson1.print();
		
		System.out.print(space);
		
		SalesPerson salesPerson2 = new SalesPerson();
		salesPerson2.setFirstName("Robert");
		salesPerson2.setLastName("Stack");
		salesPerson2.print();
	}
	

}
